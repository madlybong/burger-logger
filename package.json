{
  "name": "@astrake/burger-logger",
  "version": "0.0.10",
  "description": "Fast async non-blocking logger for Bun",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test --coverage && bun run typecheck",
    "test:watch": "bun test --watch",
    "dev": "bun run test:watch",
    "version:patch": "npm version patch && git push --follow-tags",
    "version:minor": "npm version minor && git push --follow-tags",
    "version:major": "npm version major && git push --follow-tags",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "bun run test && bun run build",
    "publish": "npm publish --access public"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "bun": "^1.3.0"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "bun",
    "logger",
    "typescript",
    "async",
    "logging"
  ],
  "license": "MIT",
  "author": "madlybong <madlybong@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madlybong/burger-logger.git"
  },
  "bugs": {
    "url": "https://github.com/madlybong/burger-logger/issues"
  },
  "homepage": "https://github.com/madlybong/burger-logger#readme"
}
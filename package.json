{
  "name": "@astrake/burger-logger",
  "version": "0.0.30",
  "description": "Fast async non-blocking logger for Bun",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "bun test tests/*.ts --coverage && bun run typecheck",
    "test:watch": "bun test tests/*.ts --watch",
    "dev": "bun run test:watch",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "publish": "bun scripts/publish.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "bun": "^1.3.0"
  },
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "keywords": [
    "bun",
    "logger",
    "typescript",
    "async",
    "logging"
  ],
  "license": "MIT",
  "author": "Anuvab Chakraborty <madlybong@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madlybong/burger-logger.git"
  },
  "bugs": {
    "url": "https://github.com/madlybong/burger-logger/issues"
  },
  "homepage": "https://github.com/madlybong/burger-logger#readme"
}